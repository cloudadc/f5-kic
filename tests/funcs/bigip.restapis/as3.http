### cm-as3-canary-template
POST https://10.250.15.180/mgmt/shared/appsvcs/declare?show=full&controls.logLevel=debug
Authorization: Basic admin:P@ssw0rd123

{
    "class": "AS3",
    "action": "deploy",
    "persist": true,
    "declaration": {
        "class": "ADC",
        "schemaVersion": "3.19.0",
        "id": "cebbank-configmap-k8s-ceb001",
        "remark": "HTTP application",
        "k8s-ceb001": {
            "class": "Tenant",
            "k8s-ceb001-svc-1-app": {
                "class": "Application",
                "template": "generic",
                "k8s-ceb001-svc-1-vs-80": {
                    "maxConnections": 123,
                    "nat64Enabled": false, 
                    "class": "Service_HTTP",
                    "virtualAddresses": [
                        "192.168.5.91"
                    ],
                    "virtualPort": 80,
                    "profileHTTP": {
                        "use": "http_X-Forwarded-For"
                    },
                    "persistenceMethods": [
                        {
                            "use": "cookie_encryption"
                        }
                    ],
                    "iRules": [
                        "irule_k8s-ceb001-svc-1"
                    ],
                    "pool": "k8s-ceb001-svc-1-pool-8080"
                },
                "irule_k8s-ceb001-svc-1": {
                    "class": "iRule",
                    "remark": "switch between pools",
                    "iRule": "when HTTP_REQUEST {\n if { [HTTP::uri] starts_with \"/coffee\" } {\n pool /k8s-ceb001/k8s-ceb001-svc-1-app/k8s-ceb001-svc-1-pool-8080 \n } else { \n pool /k8s-ceb001/k8s-ceb001-svc-1-app/k8s-ceb001-app-svc-4-pool-8080 \n } \n}"
                },
                "cookie_encryption": {
                    "class": "Persist",
                    "persistenceMethod": "cookie",
                    "encrypt": true,
                    "cookieMethod": "insert",
                    "passphrase": {
                        "ciphertext": "a3RjeGZ5Z2Q=",
                        "protected": "eyJhbGciOiJkaXIiLCJlbmMiOiJub25lIn0="
                    }
                },
                "http_X-Forwarded-For": {
                    "class": "HTTP_Profile",
                    "xForwardedFor": true
                },
                "k8s-ceb001-svc-1-http-8080": {
                    "class": "Monitor",
                    "monitorType": "http",
                    "send": "GET /coffee/ HTTP/1.1\r\nHost:f5.monitor.com\r\n\r\n",
                    "interval": 5,
                    "timeout": 16,
                    "receive": "coffee"
                },
                "k8s-ceb001-app-svc-4-pool-8080": {
                    "class": "Pool",
                    "monitors": [
                        "tcp"
                    ],
                    "minimumMembersActive": 0,
                    "members": [
                        {
                            "servicePort": 80,
                            "serverAddresses": []
                        }
                    ]
                },
                "k8s-ceb001-svc-1-pool-8080": {
                    "class": "Pool",
                    "monitors": [
                        {
                            "use": "k8s-ceb001-svc-1-http-8080"
                        }
                    ],
                    "loadBalancingMode": "least-connections-member",
                    "minimumMembersActive": 0,
                    "members": [
                        {
                            "servicePort": 80,
                            "serverAddresses": []
                        }
                    ]
                }
            }
        }
    }
}


